import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int[] arr;
		int arr1[];
		int []arr2;
		
		arr = new int[10]; // tạo mảng int có 10 phần tử
		for (int i = 0; i < arr.length; i++) {
			System.out.println("Nhập phần tử thứ " + i + ": ");
			arr[i] = sc.nextInt();
		}
	}
}
